{
    "name": "vue-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lint": "vue-cli-service lint --no-fix",
        "lint:fix": "vue-cli-service lint",
        "serve": "vue-cli-service serve",
        "serve:prod": "node server.js",
        "clean:wwwroot": "del-cli --force ../wwwroot/**/*",
        "clean:dist": "del-cli ./dist/**/*",
        "clean:all": "npm run clean:wwwroot && npm run clean:dist",
        "dist:deploy": "npm run clean:wwwroot && cpy --parents --cwd=dist **/* !index.html ../../wwwroot",
        "build": "vue-cli-service build --no-clean --skip-plugins pwa,workbox",
        "build:full": "npm run clean:dist && npm run build",
        "build:deploy": "npm run build:full && npm run dist:deploy",
        "test:unit": "vue-cli-service test:unit",
        "test:unit:watch": "vue-cli-service test:unit --watchAll",
        "test:e2e": "vue-cli-service test:e2e"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "chart.js": "^2.9.4",
        "compression": "^1.7.4",
        "core-js": "^3.6.5",
        "dayjs": "^1.10.4",
        "express": "^4.17.1",
        "register-service-worker": "^1.7.1",
        "vue": "^2.6.11",
        "vue-chartjs": "^3.5.1",
        "vue-class-component": "^7.2.3",
        "vue-i18n": "^8.23.0",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.2.0",
        "vuelidate": "^0.7.6",
        "vuetify": "^2.4.5",
        "vuex": "^3.4.0"
    },
    "devDependencies": {
        "@types/vuelidate": "^0.7.13",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-e2e-cypress": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-pwa": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-typescript": "^7.0.0",
        "@vue/test-utils": "^1.0.3",
        "cpy-cli": "^3.1.1",
        "cypress": "^6.5.0",
        "del-cli": "^3.0.1",
        "eslint": "^7.20.0",
        "eslint-plugin-vue": "^7.6.0",
        "jest": "^26.6.3",
        "sass": "^1.32.0",
        "sass-loader": "^10.0.0",
        "typescript": "^4.2.2",
        "vue-cli-plugin-i18n": "~1.0.1",
        "vue-cli-plugin-vuetify": "~2.1.0",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.7.0"
    }
}
